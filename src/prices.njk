---
title: Wedding Music Price List
mainHeading: Wedding Music Price List
dividerWidth: 50%
fontFamily: "'Playpen Sans', sans-serif"
bgColor: "oklch(0.98 0.01 120)"
textColor: "oklch(0.25 0.02 10)"
headingColor: "oklch(0.2 0.01 20)"
sectionHeadingColor: "oklch(0.3 0.02 30)"
borderColor: "oklch(0.2 0.02 20)"
imageUrl: "https://assets.codepen.io/170881/flowers_wetercolor.png"


description: |
  We offer a variety of wedding music services to make your special day unforgettable. 
  Our experienced musicians will work with you to create the perfect atmosphere for your ceremony and reception.
descriptionPoints:
  - "3~4 songs before the wedding ceremony"
  - "Music: entrance, psalm, communion, ring exchange, signatory, exit"
  - "Music selected from our established list of repertoire"

services:

  - section: "Our Services"
    items:
      - name: "CALLIGRAPHY SERVICES"
        price: "$35"
        
      - name: "ENVELOPE ADDRESSING"
        price: "$50"
        
      - name: "PLACE CARDS"
        price: "$45"
        
      - name: "QUOTES & POEMS"
        price: "$50"
        
  - section: "Additional Services 2"
    items:
      - name: "CUSTOM MONOGRAM"
        price: "$35"
        
      - name: "WEDDING PROGRAM"
        price: "$50"
        
      - name: "THANK YOU CARDS"
        price: "$45"
        
      - name: "DIGITAL FILES"
        price: "$50"
        
  - section: "Additional Services 3"
    items:
      - name: "CUSTOM MONOGRAM"
        price: "$35"
        
      - name: "WEDDING PROGRAM"
        price: "$50"
        
      - name: "THANK YOU CARDS"
        price: "$45"
        
      - name: "DIGITAL FILES"
        price: "$50"
        
---

<!DOCTYPE html>
<html>
  <head>
    <!-- Include Startr Style CSS -->
    <link rel="stylesheet" href="https://startr.style/style.css">
    <style>
      @font-face {
        font-family: "Westonia";
        src: url("https://assets.codepen.io/170881/Westonia.otf") format("opentype");
        font-display: swap;
      }

      @font-face {
        font-family: "Playpen Sans";
        src: url("https://assets.codepen.io/170881/PlaypenSans.ttf") format("truetype");
        font-display: swap;
      }

      body {
        font-family: "Playpen Sans", sans-serif;
      }

      {
        font-family: "Westonia", "Brush Script MT",;
      }
    </style>
  </head>

  <body style="--ff: sans-serif;;  --bgc:oklch(0.98 0.01 120); --c:oklch(0.25 0.02 10); --p:2em; --d:flex; --g:1em; --maxw:60rem; --m:auto;">
    <img alt="Watercolor floral design" src="https://assets.codepen.io/170881/flowers_wetercolor.png" style="--d:none; --d-md:bloc;--radius:12px; --w-sm:50%; --rotate: 6deg;--ml: -30%;--mr: -30%;">

    <our-content style="--m:auto">
      <section style="--d:flex; --fd:column; --ai:center; text-align:center; gap:1em; --maxw:40rem; margin:0 auto;">
        <h1 style="--ff: 'Westonia', 'Brush Script MT',cursive; --size:5em; --c:{{ headingColor }};">{{ mainHeading }}</h1>
        <hr style="--w:{{ dividerWidth }}; --m:1em auto;">
      </section>
      <section style="--d:flex; --fd:column; --ai:center; text-align:center; gap:1em; --maxw:30rem; margin:0 auto;">
        <!--<p style="--size:1.5em; --c:{{ textColor }};">{{ description }}</p>-->
        <p style="--c:{{ textColor }}; --ta:left">
          {{ description }}
        </p>
        <p style="--c:{{ textColor }}; --ta:left">
          {% for desc in descriptionPoints %}
            {{ desc }}<br>
          {% endfor %}
        </p>

      {% for serviceSection in services %}
        <section style="--maxw:40rem; margin:2em auto; --p:1em;">
          <h2 style="--ff: 'Westonia', 'Brush Script MT',cursive; --size:3em; --c:{{ sectionHeadingColor }}; text-align:right;">{{ serviceSection.section }}</h2>
          <div style="--d:grid; --gtc:1fr auto; --size:1em; --ai:center;">
            {% for item in serviceSection.items %}
              <div style="--p:0 1em 0 0;">{{ item.name }}</div>
              <div style="--p:.3em 0 0 0; --pl:1em; --bl:2px solid {{ borderColor }}; --ta:right; --weight:bold;">{{ item.price }}</div>
            {% endfor %}
          </div>
        </section>
      {% endfor %}
    </our-content>
  </body>
</html>